{

  "AllowedHosts": "*",
  "Database": {
  "Provider": "sqlserver"
},
  "Cors": {
    "AllowedOrigins": [
      "https://app.yourcompany.com",
      "https://api.banking.yourcompany.com"
    ]
  },
  "RateLimiting": {
    "PermitLimit": 100,
    "WindowMinutes": 1
  },
  "OpenTelemetry": {
    "UseAzureMonitor": true
  },
 
    "ApplicationInsights": {
    "ConnectionString": "InstrumentationKey=your-production-instrumentation-key"
  } 
}
